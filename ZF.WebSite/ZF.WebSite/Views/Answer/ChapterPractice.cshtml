@{
    ViewBag.Title = "课程章节练习";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .contentIn {
        background: #f3f3f3;
    }

    .chooseItem .col-xs-12 label {
        cursor: pointer;
        position: relative;
        font-family: "微软雅黑";
        color: black;
        font-size: 14px;
        min-width: 600px;
        padding: 3px;
    }

    .chooseItem .col-xs-6 label {
        font-weight: 200;
        cursor: pointer;
        position: relative;
        color: black;
        font-size: 16px;
    }

    .chooseItem .col-xs-6 {
        padding: 10px;
    }

    .chooseItem .col-xs-12 {
        padding-left: 10px;
        width: 98%;
        float: left;
        position: relative;
        min-height: 1px;
        padding-right: 15px;
        line-height:30px;
        margin-top:6px;
    }

    #container {
        width: 1200px;
        min-width: 640px;
        max-width: 1200px;
        width: auto;
        margin: 20px auto;
        overflow:hidden;
    }
    /* å·¦ä¾§è¯•å·å†…å®¹ */
    .ud-main {
        width: 910px;
        margin-right: 0px;
        float: left;
        _display: inline;
        padding: 20px 30px;
        background:#fff;
    }
        /* è¯•å·æ ‡é¢˜éƒ¨åˆ† */
        .ud-main h2.ud-title {
            width: 100%;
            height: 60px;
            line-height: 60px;
            padding-top: 20px;
            text-align: center;
            position: relative;
        }

            .ud-main h2.ud-title .editor_titles {
                border: none;
                height: 40px;
                line-height: 40px;
            }

            .ud-main h2.ud-title input.editor_titles {
                width: 90%;
                text-align: center;
                font-weight: bold;
                font-size: 24px;
                background: #f0f0f0;
                display: none;
            }

            .ud-main h2.ud-title span.editor_titles {
                display: inline-block;
                width: 95%;
                height: 60px;
                line-height: 60px;
                font-weight: lighter;
                font-size: 22px;
                letter-spacing: 2px;
                overflow: hidden;
                white-space: nowrap;
                text-overflow: ellipsis;
                -o-text-overflow: ellipsis;
                -icab-text-overflow: ellipsis;
                -khtml-text-overflow: ellipsis;
                -moz-text-overflow: ellipsis;
                -webkit-text-overflow: ellipsis;
            }

            .ud-main h2.ud-title a.ud-edtior {
                position: absolute;
                right: 0;
                bottom: 15px;
                display: inline-block;
                width: 19px;
                height: 19px;
                background: url(../images/ud-tit-icon.png) no-repeat center center;
                margin-left: 20px;
            }

    .ud-info {
        font-size: 20px;
        text-align: center;
        line-height: 20px;
        margin: 20px auto;
    }

        .ud-info b {
            margin-right: 10px;
            font-weight: normal;
        }

        .ud-info a {
            display: inline-block;
            padding: 5px 10px 5px 20px;
        }

    .ud-questionTypes {
        font-size: 16px;
        line-height: 30px;
        font-weight: bold;
        background: #f3f3f3;
        padding: 8px;
        color: #4c4a4a;
    }


    #ud-keep-paper {
        padding: 10px 50px;
        font-size: 18px;
        font-weight: normal;
        color: #fff;
        background: #f32426;
        border-radius: 3px;
        display: block;
        text-align: center;
        margin: 10px;
    }
    /* å³ä¾§ç»„å·æŒ‰é’® */
    .the-right {
        /* width: 200px; */
        float: left;
        _display: inline;
        position: relative;
        padding-top: 80px;
    }

    .right-fixed {
        /* height: 446px; */
        position: fixed;
        z-index: 9;
        top: 202px;
        background:#fff;border-radius:4px;
        overflow:hidden;
        width: 280px;
        margin-left:20px;
        border: 1px solid #e2e2e2;
        box-shadow: 1px 1px 4px #ccc;
        top: 50%;
        margin-top: -128px;
    }

    .ud-tool-btn {
        padding: 0 0 0 60px;
    }

        .ud-tool-btn a {
            display: block;
            width: 160px;
            height: 40px;
            text-align: center;
            line-height: 40px;
            font-size: 18px;
            font-weight: lighter;
            color: #fff !important;
            border-radius: 2px;
            margin-bottom: 10px;
        }

        .ud-tool-btn .ud-print {
            background: #f7645c;
        }

        .ud-tool-btn .ud-download {
            background: #4cad51;
        }

        .ud-tool-btn .ud-record {
            background: #5b95d8;
        }

        .ud-tool-btn .ud-choose {
            background: #665659;
        }

        .ud-tool-btn .ud-empty {
            background: #90c03e;
        }

    .downBox{   
        padding: 20px;
        max-height: 240px;
        overflow: hidden;
        overflow-y: auto;

    }
    .btn{
    background: #f32426;
    color: #fff;
    padding: 4px 10px;
    margin-left: 10px;
    border-radius: 3px;}
    .btn:hover{color:#f9f9f9;}
    .top{padding:10px;border-bottom:1px dashed #d9d9d9;}
        .tn-time #minute_show, .tn-time #second_show {
            display: inline-block;
            height: 36px;
            line-height: 36px;
            width: 45px;
            height: 36px;
            text-align: center;
            font-size: 20px;
            border-radius: 3px;
            background: #4599b1;
            color: #fff;
            margin: 0 3px;
            vertical-align: bottom;
        }

    .tn-ordinal {
        width: 100%;
        background: #9ed4e3;
    }

    .inputType input {
        float: left;
        margin: 0.6em;
    }

    .done-high {
       /* background: #d1d1d1 !important; /* url(http://static.vko.cn/vko/images/tiku/tknum_bg.png) left -60px no-repeat!important; 
         color: #575757!important; */
       background:#f32426;
       border-color:#f32426;
       color:#ffffff;
       font-size:12px;
    }

    .checked {
        background: #FFAB20 !important; /* url(http://static.vko.cn/vko/images/tiku/tknum_bg.png) left -30px no-repeat!important; */
        /* color: #4599b1!important; */
    }
    /* ä¸‰è§’ç®­å¤´ */
    #triangle {
        width: 20px;
        height: 20px;
        position: absolute;
        left: -20px;
        top: 30px;
    }

        #triangle i {
            position: absolute;
            top: 0;
            border-width: 10px;
            border-style: dashed solid dashed dashed;
            border-color: transparent #9ED4E3 transparent transparent;
        }

    .orders {
        float: left;
        line-height: 30px;
        font-weight: bold;
        padding-right: 5px;
    }

    /*
    #single{
        background: none;
    }*/
    #single_ordinal,
    #multi_ordinal,
    #fillblank_ordinal,
    #answer_ordinal,
    #closetest_ordinal,
    #reading_ordinal,
    #write_ordinal {
        display: none;
    }

    .quest_op {
        width: 43%;
        padding: 10px 4% 5px 15px;
        display: inline-block;
        vertical-align: top;
        position: relative;
    }

    /*input[type="radio"], input[type="checkbox"] {
        margin: 0px 0 0 !important;
    }*/
    .box{
        border: 1px solid #e0e3e9;
        display: inline-block;
        padding: 10px;
        margin-top: 5px;
    }
    .box:hover{box-shadow:1px 2px 5px #e6e6e6;}
    .tn-ordinal-num{margin-bottom:10px;overflow:hidden;}
    .tn-ordinal-num span{    
        background: #f3f3f3;
        padding: 4px 10px;
        height:25px;line-height:25px;
        border-radius: 3px;
        color: #151515;
    }
    .topic a{width:26px;height:26px;line-height:26px;border:1px solid #e5e5e5;text-align:center; display:inline-block;margin:0 10px 10px 0;}
    .box>.col-xs-12{margin-bottom:25px;}
</style>
<div id="container" class="clearfix">
    <div class="ud-main clearfix">
        <input type="hidden" Id="ChapterId" value="@ViewBag.ChapterId" />
        <input type="hidden" Id="ChapterQuestionsId" value="@ViewBag.ChapterQuestionsId" />
        @*<h2 class="ud-title">不知道干嘛用的，白占空间，先注释
            <input class="editor_titles" type="text" value="" placeholder="">
            <span class="editor_titles" Id="ChapterTiele"></span>
            <a class="ud-edtior" href="javascript:void(0);"></a>
        </h2>*@

        <div class="ud-info">
            @*<span>练习时间：</span>
            <b id="DateTime"></b>*@
            @*<span>名称：</span>*@
            <b value="51" id="CourseName"></b>
            <span>视频名称：</span>
            <b value="22" id="ChapterName"></b>
            <span>题数：</span>
            <b id="Count"></b>
        </div>
        <div class="chooseItem anli" id="ChapterPractice">
        </div>
       
    </div>
    <div class="the-right">
        <div class="right-fixed">
            <div class="top" style="">现在用时：<span class="tn-time red"></span><a class="btn" id="Toview"> 查看练习记录</a></div>
            <div class="downBox" id="tn-ordinal">

            </div>
            <div class="ud-keep-paper">
                <a id="ud-keep-paper" href="javascript:savePaper();">提交试卷</a>
            </div>
        </div>
    </div>
</div>
<script src="@HttpContext.Current.Application["DefuleDomain"]/Views/Answer/ChapterPractice.js?v=1.0"></script>
