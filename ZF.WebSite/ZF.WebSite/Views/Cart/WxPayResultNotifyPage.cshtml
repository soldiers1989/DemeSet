<input type="hidden" value="@ViewBag.isTrue" id="isTrue" />
<input type="hidden" value="@ViewBag.orderNo" id="orderNo" />
<input type="hidden" value="@ViewBag.price" id="price" />
<script type="text/javascript">
    $(function () {
        var istrue = $("#isTrue").val();
        var obj = new Object();
        obj.OrderNo = $("#orderNo").val();
        obj.OrderAmount = $("#price").val();
        if (istrue) {
            obj.State = 0;
        } else {
            obj.State = 1;
        }
        topevery.ajaxwx({
            url: "api/Sheet/EnditSheetStates",
            data: JSON.stringify(obj),
            async: false
        }, function (data) {
            var dataInfo = data.Result;
            if (dataInfo.Success) {
                window.location.href = "/LearningCenter/Index";
            } else {
                window.location.href = "/LearningCenter/Index";
            }
        });
    })
</script>