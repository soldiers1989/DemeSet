
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form id="myForm" method="post" target="myFrame">
    <input type="hidden" name="parment" value="@ViewBag.parment" id="data_param" />
</form>
<iframe name="myFrame" width="100%" height="1150"  frameborder="0" scrolling="no"></iframe>
<script type="text/javascript">
    $(function () {
        $("#myForm").attr("action", "SubmitOrder");
        $("#myForm").submit();
    })
</script>