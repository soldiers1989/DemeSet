
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="black" name="apple-mobile-web-app-status-bar-style">
    <meta content="telephone=no" name="format-detection">
    <meta content="email=no" name="format-detection">
    <meta http-equiv="Cache-Control" CONTENT="no-cache">
    <meta http-equiv="pragram" content="no-cache">
    <link href="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/resources/style/index1.css" rel="stylesheet" type="text/css">
    <title>订单详情</title>

</head>
<body>
    <div class="jrzf-box">
        <div id="detailinfo"></div>
        <script type="text/html" id="orderDetail_html">
            <div class="dingd_nav"><p>订单号：{{OrderNo}}</p></div>
            <div class="dingd_list01">
                <h4>付款信息</h4>
                <ul>
                    <li>
                        付款方式：
                        <span>
                            {{if !Name}}
                            未支付
                            {{else}}
                            {{Name}}
                            {{/if}}
                        </span>
                    </li>
                    <li>
                        付款时间：
                        <span>
                            {{if !PayTime}}
                            无
                            {{else}}
                            {{PayTime}}
                            {{/if}}
                        </span>
                    </li>
                    <li>商品总额：<span>{{CommodityPrice}}</span></li>
                    <li>支付金额：<span>{{FactPayAmount}}</span></li>
                </ul>
            </div>

            <div class="dingd_list01">
                <h4>发票信息</h4>
                <ul>
                    <li>
                        发票抬头：<span>
                            {{if !InvoiceHeader}}
                            无
                            {{else}}
                            {{InvoiceHeader}}
                            {{/if}}
                        </span>
                    </li>
                    <li>
                        纳税人标识号：<span>
                            {{if !TaxpayerIdentificationNumber}}
                            无
                            {{else}}
                            {{TaxpayerIdentificationNumber}}
                            {{/if}}
                        </span>
                    </li>
                    <li>
                        手机号：<span>
                            {{if !InvoicePhone}}
                            无
                            {{else}}
                            {{InvoicePhone}}
                            {{/if}}
                        </span>
                    </li>
                    <li>
                        发票邮箱：<span>
                            {{if !InvoiceMailbox}}
                            无
                            {{else}}
                            {{InvoiceMailbox}}
                            {{/if}}
                        </span>
                    </li>
                    <li>
                        发票开具状态：<span>
                            {{if InvoiceState==1}}
                            已开具
                            {{else}}
                            未开具
                            {{/if}}
                        </span>
                    </li>
                    <li>电子发票将在订单完成后1-2天内开具</li>
                </ul>
            </div>

            <div class="dingd_list01">
                <div class="dingd_tit">
                    <span class="tit_01"><em style="padding-left: 1em;">商品</em></span>
                    <span class="tit_02">单价</span>
                    <span class="tit_03">商品数量</span>
                    <div class="clear"></div>
                </div>
                {{if sheetmodelpagedetailoutput!=null}}
                {{each sheetmodelpagedetailoutput}}
                <div class="dingd_nr">
                    <div class="dingd_nr01">
                        <div class="dingd_sp">
                            <div class="dingd_sp-l"> <img src="{{$value.CourseIamge}}" title="{{$value.CourseName}}" alt=""> </div>
                            <div class="dingd_sp-r">
                                <p title="{{$value.CourseName}}">{{$value.CourseName}}</p>

                            </div>
                            <div class="clear"></div>
                        </div>

                    </div>
                    <div class="dingd_nr02">{{$value.FavourablePrice}}</div>
                    <div class="dingd_nr03">{{$value.Num}}</div>
                    <div class="clear"></div>
                </div>
                {{/each}}
                {{else}}
                暂无数据
                {{/if}}
            </div>
            <div class="hei1em"></div>
            <div class="dingd_je">
                <dl>
                    <dt>商品总额：{{CommodityPrice}}</dt>
                    <dd>
                        应付总额：
                        {{if State==0}}
                        <span class="txt count">{{FactPayAmount}}</span>
                        {{else}}
                        {{OrderAmount}}
                        {{/if}}
                    </dd>
                </dl>
            </div>
            <div class="line_3px"></div>
         
        </script>
        <div id="electronicInvoice">
            <div class="jrzf-list08">
                <h5>
                    <input type="checkbox" id="openinvoice" name="sex" class="jdradio001">
                    <label for="openinvoice">开具电子发票</label>
                </h5>
                <div class="container" style="display:none;">
                    @*<ul class="tabs">
                        <li id="lione" class="active"><a href="#tab1">个人</a></li>
                        <li id="liall"><a href="#tab4">企业</a></li>
                        <div class="clear"></div>
                    </ul>*@
                    <div class="tab_container">
                        <div id="tab1" class="tab_content" style="display: block; ">
                            <dl>
                                <dt>发票抬头</dt>
                                <dd>
                                    <input type="text" id="txtInvoiceHeader">
                                </dd>
                                <div class="clear"></div>
                            </dl>
                            <dl id="divpersonal" >
                                <dt>纳税人识别号</dt>
                                <dd>
                                    <input type="text" id="txtTaxpayerIdentificationNumber">
                                </dd>
                                <div class="clear"></div>
                            </dl>
                            <dl>
                                <dt><b>*</b>手机号</dt>
                                <dd>
                                    <input type="text" id="txtInvoicePhone">
                                </dd>
                                <div class="clear"></div>
                            </dl>
                            <dl>
                                <dt><b>*</b>邮箱地址</dt>
                                <dd>
                                    <input type="text" placeholder="该邮箱为接收电子发票地址" id="txtInvoiceMailbox">
                                </dd>
                                <div class="clear"></div>
                            </dl>
                        </div>
                    </div>
                </div>
            </div>
            <div id="goodssavefp" class="dingd_fpiao" style="display:none;">
                <a href="javascript:;" onclick="addInvoiceInfo()">保存发票信息</a>
                <div class="clear"></div>
            </div>
        </div>
    </div>
    <script src="http://code.jquery.com/jquery-2.2.3.min.js?v=1.0"></script>
    <script src="@HttpContext.Current.Application["DefuleDomain"]/Images/Home/js/ajax.js" type="text/javascript"></script>
    <script src="@HttpContext.Current.Application["DefuleDomain"]/Libs/layer/js/layer.js?v=1.0"></script>
    <script src="@HttpContext.Current.Application["DefuleDomain"]/Js/jquery.cookie.js?v=1.0"></script>
    <script src="@HttpContext.Current.Application["DefuleDomain"]/Js/base.js?v=1.0"></script>
    <script src="@HttpContext.Current.Application["DefuleDomain"]/Js/template.js?v=1.0"></script>
    <script src="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/Views/PersonalCenter/OrderDetail.js?v=1.0"></script>
</body>
</html>