@{
    Layout = "~/Areas/jjs/Views/Shared/_Layout.cshtml";
}
<html>
<head>
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="black" name="apple-mobile-web-app-status-bar-style">
    <meta content="telephone=no" name="format-detection">
    <meta content="email=no" name="format-detection">
    <meta http-equiv="Cache-Control" CONTENT="no-cache">
    <meta http-equiv="pragram" content="no-cache">
    <link href="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/resources/style/user.css" rel="stylesheet" type="text/css">
    <title>收货地址</title>
</head>
<body>
    <div id="bindinfo">
       
    </div>
    <script type="text/html" id="path_html">
        {{if length>0}}
        {{each}}
        <div class="shdz_list">
            <div class="shdz_name">
                <h3>{{$value.Contact}}<span>{{$value.ContactPhone}}</span></h3>
                <p>{{$value.Province}}-{{$value.City}}-{{$value.Town}}-{{$value.DetailedAddress}}</p>
            </div>
            <div class="shdz_tb">
                <div class="shdz_tb_l">
                    {{if $value.DefaultAddress===1}}
                    <i style="color:red;">默认地址</i>
                    {{else}}
                    <a name="{{$value.Id}}" onclick="makeAddressAllDefaultByoverseas(this)" href="javascript:;">设为默认地址</a>
                    {{/if}}
                </div>
                <div class="shdz_tb_r">
                    <span><img src="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/resources/images/bianji02@2x.png"><a href="/jjs/PersonalCenter/MyHandOutPathEdit?Id={{$value.Id}}">编辑</a></span>
                    <span><img src="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/resources/images/bianji01@2x.png"><a name="{{$value.Id}}" onclick="alertDelAddressDiag(this);" href="javascript:;">删除</a></span>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        {{/each}}
        {{else}}
        <div class="zanwu">
            <img src="/Areas/jjs/resources/images/zanwu.png">
            <p><a href="#">暂无记录</a></p>
        </div>
        {{/if}}
    </script>
    <div class="shdz_anniu"><a href="/jjs/PersonalCenter/MyHandOutPathEdit">添加新地址</a></div>
    <div class="hei5em"></div>
</body>
</html>
<script src="@HttpContext.Current.Application["DefuleDomain"]/Images/Home/js/ajax.js" type="text/javascript"></script>
<script src="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/Views/PersonalCenter/MyHandOutPath.js?v=1.0"></script>
