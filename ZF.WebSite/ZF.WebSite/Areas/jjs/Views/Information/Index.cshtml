
@{
    Layout = "~/Areas/jjs/Views/Shared/_Layout.cshtml";
}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="black" name="apple-mobile-web-app-status-bar-style">
    <meta content="telephone=no" name="format-detection">
    <meta content="email=no" name="format-detection">
    <meta http-equiv="Cache-Control" CONTENT="no-cache">
    <meta http-equiv="pragram" content="no-cache">
    <link href="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/resources/style/index1.css" rel="stylesheet" type="text/css">
    <title>资讯分类</title>
</head>
<body>
    <input id="PageIndex" value="1" type="hidden" />
    <input id="Rows" value="5" type="hidden" />

    <div class="zxlb-nav">
        <ul id="GetDataType"></ul>
        <script type="text/html" id="GetDataType_html">
            {{each Result}}
            <li><a class="{{$value.Id}}" onclick="GetClassList('{{$value.Id}}')">{{$value.DataTypeName}}</a></li>
            {{/each}}
            <div class="clear"></div>
        </script>
    </div>

    <!--最新资讯开始-->
    <div class="zxlb-zixun" id="Affiche">
        <script type="text/html" id="Affiche_html">
            {{each Rows}}
            <a href="/jjs/Information/Detail?Id={{$value.Id}}">
                <div class="zxlb-zx-rl">
                    <div class="zxlb-zx-rl-l">
                        <h4>
                            {{$value.Title}}
                        </h4>
                        <dl>
                            <dd>{{$value.AddTime}}</dd>
                            @*<dt>88浏览</dt>*@
                            <div class="clear"></div>
                        </dl>
                    </div>
                    <div class="zxlb-zx-rl-r">
                        <img src="{{$value.AfficheIamge}}">
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </a>{{/each}}
        </script>
    </div>
    <!--最新资讯结束-->
    <!--底部导航开始-->
    <div class="header-nav">
        <ul>
            <li><a href="~/jjs/HomePage/Index" class="img01">首页</a></li>
            <li style="display: none;" class="hover"><a  href="~/jjs/Information/Index" class="img02">最新资讯</a></li>
            <li><a href="~/jjs/PersonalCenter/Index" class="img03">个人中心</a></li>
        </ul>
    </div>
    <!--底部导航结束-->

</body>
</html>
<script src="@HttpContext.Current.Application["DefuleDomain"]/Images/Home/js/ajax.js" type="text/javascript"></script>
<script src="@HttpContext.Current.Application["DefuleDomain"]/Areas/jjs/Views/Information/Index.js?v=1.0"></script>
