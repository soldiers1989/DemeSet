/* Table: t_Paper_StructureDetail                               */
/*==============================================================*/
create table t_Paper_StructureDetail (
   Id                   nvarchar(36)         not null,
   StuctureId           nvarchar(36)         not null,
   QuesitonTypeName     nvarchar(50)         not null,
   QuestionType         nvarchar(36)         null,
   QuestionClass        nvarchar(36)         null,
   DifficultLevel       int                  null,
   QuestionCount        int                  not null,
   QuestionTypeScoreSum int                  not null,
   OrderNo              int                  not null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
