/*==============================================================*/
/* Table: t_Base_UserLoginLog                                   */
/*==============================================================*/
create table t_Base_UserLoginLog (
   Id                   nvarchar(36)         not null,
   UserId               nvarchar(36)         null,
   LoginTime            datetime             null,
   LoginIp              nvarchar(20)         null,
   LoginType            nvarchar(36)         null,
   constraint PK_T_BASE_USERLOGINLOG primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Base_UserLoginLog') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
execute sp_dropextendedproperty 'MS_Description',  
   'user', @CurrentUser, 'table', 't_Base_UserLoginLog' 
 
end 


select @CurrentUser = user_name() 
execute sp_addextendedproperty 'MS_Description',  
   '用户登录日志表', 
   'user', @CurrentUser, 'table', 't_Base_UserLoginLog'
go

