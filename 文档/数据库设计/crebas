/*==============================================================*/
/* Table: t_Course_Resource                                     */
/*==============================================================*/
create table t_Course_Resource (
   Id                   nvarchar(36)         not null,
   ResourceName         nvarchar(100)        not null,
   ResourceUrl          nvarchar(200)        not null,
   ResourceSize         nvarchar(20)         null,
   CourseId             nvarchar(36)         null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(50)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(50)         null,
   IsDelete             int                  not null,
   constraint PK_T_COURSE_RESOURCE primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Course_Resource') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
execute sp_dropextendedproperty 'MS_Description',  
   'user', @CurrentUser, 'table', 't_Course_Resource' 
 
end 


select @CurrentUser = user_name() 
execute sp_addextendedproperty 'MS_Description',  
   '课程资源管理', 
   'user', @CurrentUser, 'table', 't_Course_Resource'
go

