/*==============================================================*/
/* Table: t_Paper_Detatail                                      */
/*==============================================================*/
create table t_Paper_Detatail (
   Id                   nvarchar(36)         not null,
   PaperId              nvarchar(36)         not null,
   QuestionId           nvarchar(36)         not null,
   QuestionTypeId       nvarchar(36)         not null,
   QuestionScore        numeric(5,2)         not null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_PAPER_DETATAIL primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Paper_Detatail') and minor_id = 0)
