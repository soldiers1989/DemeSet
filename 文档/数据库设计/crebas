/*==============================================================*/
/* Table: t_Base_Menu                                           */
/*==============================================================*/
create table dbo.t_Base_Menu (
   Id                   nvarchar(36)         not null,
   ModuleId             nvarchar(36)         null,
   MenuName             nvarchar(50)         null,
   Url                  nvarchar(100)        null,
   Sort                 int                  null,
   Class                nvarchar(20)         null,
   Description          nvarchar(200)        null,
   constraint PK_Sys_Menu primary key (Id)
         on "PRIMARY"
)
on "PRIMARY"
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('dbo.t_Base_Menu') and minor_id = 0)
begin 
   execute sp_dropextendedproperty 'MS_Description',  
   'user', 'dbo', 'table', 't_Base_Menu' 
 
end 


execute sp_addextendedproperty 'MS_Description',  
   '菜单表', 
   'user', 'dbo', 'table', 't_Base_Menu'
go

if exists(select 1 from sys.extended_properties p where
      p.major_id = object_id('dbo.t_Base_Menu')
  and p.minor_id = (select c.column_id from sys.columns c where c.object_id = p.major_id and c.name = 'Id')
)
begin
   execute sp_dropextendedproperty 'MS_Description', 
   'user', 'dbo', 'table', 't_Base_Menu', 'column', 'Id'

end


execute sp_addextendedproperty 'MS_Description', 
   '主键编号',
   'user', 'dbo', 'table', 't_Base_Menu', 'column', 'Id'
go

if exists(select 1 from sys.extended_properties p where
      p.major_id = object_id('dbo.t_Base_Menu')
  and p.minor_id = (select c.column_id from sys.columns c where c.object_id = p.major_id and c.name = 'ModuleId')
)
begin
   execute sp_dropextendedproperty 'MS_Description', 
   'user', 'dbo', 'table', 't_Base_Menu', 'column', 'ModuleId'

