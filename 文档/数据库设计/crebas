create table t_Course_Appraise (
   Id                   nvarchar(36)         not null,
   AppraiseCotent       nvarchar(300)        null,
   UserId               nvarchar(36)         null,
   AppraiseLevel        int                  null,
   AppraiseTime         datetime             null,
   AppraiseIp           varchar(20)          null,
   ReplyTime            datetime             null,
   ReplyContent         nvarchar(300)        null,
   ReplyAdminUserId     nvarchar(36)         null,
   CourseId             nvarchar(36)         null,
   CourseType           int                  null,
   constraint PK_T_COURSE_APPRAISE primary key (Id)
)
