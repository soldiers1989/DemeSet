/* Table: t_Course_Subject                                      */
/*==============================================================*/
create table t_Course_Subject (
   Id                   nvarchar(36)         not null,
   CourseId             nvarchar(36)         not null,
   ChapterId            nvarchar(36)         not null,
   SubjectId            nvarchar(36)         not null,
   constraint PK_T_COURSE_SUBJECT primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Course_Subject') and minor_id = 0)
