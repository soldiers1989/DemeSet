/* Table: t_Paper_PaperParam                                    */
/*==============================================================*/
create table t_Paper_PaperParam (
   Id                   nvarchar(36)         not null,
   StuctureId           nvarchar(36)         not null,
   ParamName            nvarchar(100)        not null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_PAPER_PAPERPARAM primary key (Id)
)
go

