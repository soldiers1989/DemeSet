/*==============================================================*/
/* Table: t_Base_Basedata                                       */
/*==============================================================*/
create table t_Base_Basedata (
   Id                   nvarchar(36)         not null,
   DataTypeId           nvarchar(36)         not null,
   Name                 nvarchar(50)         not null,
   Code                 nvarchar(20)         not null,
   "Desc"               nvarchar(50)         null,
   Sort                 int                  null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_BASE_BASEDATA primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Base_Basedata') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
execute sp_dropextendedproperty 'MS_Description',  
   'user', @CurrentUser, 'table', 't_Base_Basedata' 
 
end 


select @CurrentUser = user_name() 
execute sp_addextendedproperty 'MS_Description',  
   '数据字典表', 
   'user', @CurrentUser, 'table', 't_Base_Basedata'
go

if exists(select 1 from sys.extended_properties p where
      p.major_id = object_id('t_Base_Basedata')
  and p.minor_id = (select c.column_id from sys.columns c where c.object_id = p.major_id and c.name = 'Id')
)
begin
   declare @CurrentUser sysname
select @CurrentUser = user_name()
execute sp_dropextendedproperty 'MS_Description', 
   'user', @CurrentUser, 'table', 't_Base_Basedata', 'column', 'Id'

end


select @CurrentUser = user_name()
execute sp_addextendedproperty 'MS_Description', 
   '数据编码',
   'user', @CurrentUser, 'table', 't_Base_Basedata', 'column', 'Id'
go

if exists(select 1 from sys.extended_properties p where
      p.major_id = object_id('t_Base_Basedata')
  and p.minor_id = (select c.column_id from sys.columns c where c.object_id = p.major_id and c.name = 'DataTypeId')
)
begin
   declare @CurrentUser sysname
select @CurrentUser = user_name()
execute sp_dropextendedproperty 'MS_Description', 
   'user', @CurrentUser, 'table', 't_Base_Basedata', 'column', 'DataTypeId'

end


select @CurrentUser = user_name()
execute sp_addextendedproperty 'MS_Description', 
   '命名空间编号',
   'user', @CurrentUser, 'table', 't_Base_Basedata', 'column', 'DataTypeId'
go

