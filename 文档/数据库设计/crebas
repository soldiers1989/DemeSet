create table t_Course_OnTeachers (
   Id                   nvarchar(36)         not null,
   TeachersName         nvarchar(50)         not null,
   TheLabel             nvarchar(200)        null,
   Synopsis             nvarchar(100)        null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(50)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(50)         null,
   IsDelete             int                  not null,
   constraint PK_T_COURSE_ONTEACHERS primary key (Id)
)
