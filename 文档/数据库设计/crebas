/*==============================================================*/
/* Table: t_Course_Chapter                                      */
/*==============================================================*/
create table t_Course_Chapter (
   Id                   nvarchar(36)         not null,
   CapterName           nvarchar(100)        not null,
   ParentId             nvarchar(36)         null,
   CapterCode           nvarchar(50)         not null,
   CourseId             nvarchar(36)         not null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_COURSE_CHAPTER primary key (Id)
)
go

