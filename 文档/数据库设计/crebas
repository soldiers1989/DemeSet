create table t_Base_RegisterUser (
   Id                   nvarchar(36)         not null,
   UserName             nvarchar(30)         not null,
   LoginPwd             nvarchar(50)         null,
   TelphoneNum          nvarchar(50)         not null,
   AddTime              datetime             not null,
   RegisterIp           nvarchar(20)         not null,
   RegiesterType        nvarchar(36)         not null,
   State                int                  not null,
   NickNamw             nvarchar(30)         null,
   WechatId             nvarchar(200)        null,
   LastLoginTime        datetime             null,
   AreaCode             nvarchar(10)         null,
   HeadImage            nvarchar(200)        null,
   constraint PK_T_BASE_REGISTERUSER primary key (Id)
)
