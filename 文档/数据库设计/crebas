   Id                   nvarchar(36)         not null,
   ModuleName           nvarchar(50)         null,
   Class                nvarchar(50)         null,
   Sort                 int                  null,
   constraint PK_t_Sys_Module primary key (Id)
         on "PRIMARY"
)
on "PRIMARY"
