create table t_Subject_KnowledgePoint (
   Id                   nvarchar(36)         not null,
   KnowledgePointName   nvarchar(50)         not null,
   KnowledgePointCode   nvarchar(50)         not null,
   SubjectId            nvarchar(36)         not null,
   ParentId             nvarchar(36)         null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_SUBJECT_KNOWLEDGEPOINT primary key (Id)
)
