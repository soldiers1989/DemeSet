/*==============================================================*/
/* Table: t_Subject_Smallquestion                               */
/*==============================================================*/
create table t_Subject_Smallquestion (
   Id                   nvarchar(36)         not null,
   QuestionTitle        nvarchar(100)        not null,
   QuestionContent      text                 not null,
   SubjectId            nvarchar(36)         null,
   Option1              text                 null,
   Option2              text                 null,
   Option3              text                 null,
   Option4              text                 null,
   Option5              text                 null,
   Option6              text                 null,
   Option7              text                 null,
   Option8              text                 null,
   SubjectType          nvarchar(36)         not null,
   RightAnswer          nvarchar(50)         null,
   ConsultAnswer        text                 null,
   State                int                  not null,
   QuestionTextAnalysis text                 null,
   QuestionAudioAnalysis nvarchar(200)        null,
   QuestionVedioAnalysis nvarchar(200)        null,
   DigitalBookPage      nvarchar(200)        null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_SUBJECT_SMALLQUESTION primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Subject_Smallquestion') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
