/* Table: t_Course_Video                                        */
/*==============================================================*/
create table t_Course_Video (
   Id                   nvarchar(36)         not null,
   VideoName            nvarchar(200)        not null,
   VideoUrl             nvarchar(200)        not null,
   ChapterId            nvarchar(36)         not null,
   VideoLongTime        nvarchar(10)         not null,
   VideoContent         nvarchar(1000)       null,
   IsTaste              int                  null,
   TasteLongTime        int                  null,
   StudyCount           int                  null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_COURSE_VIDEO primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Course_Video') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
execute sp_dropextendedproperty 'MS_Description',  
   'user', @CurrentUser, 'table', 't_Course_Video' 
 
end 


