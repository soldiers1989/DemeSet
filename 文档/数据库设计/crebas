create table t_My_AnswerRecords (
   Id                   nvarchar(36)         not null,
   UserId               nvarchar(36)         null,
   BigQuestionId        nvarchar(36)         not null,
   SmallQuestionId      nvarchar(36)         null,
   PaperId              nvarchar(36)         not null,
   StuAnswer            nvarchar(20)         not null,
   Score                numeric(4,2)         null,
   AddTime              datetime             not null,
   constraint PK_T_MY_ANSWERRECORDS primary key (Id)
)
go

