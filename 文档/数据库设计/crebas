/*==============================================================*/
/* Table: t_Order_Sheet                                         */
/*==============================================================*/
create table t_Order_Sheet (
   Id                   nvarchar(36)         not null,
   OrderNo              nvarchar(36)         not null,
   RegisterUserId       nvarchar(36)         not null,
   AddTime              datetime             not null,
   OrderAmount          numeric(6,2)         not null,
   State                int                  not null,
   FactPayAmount        numeric(6,2)         not null,
   OrderIp              nvarchar(20)         not null,
   IsDelete             int                  not null,
   PayType              nvarchar(36)         not null,
   PayTime              datetime             null,
   OrderType            nvarchar(36)         not null,
   TradeNo              nvarchar(50)         null,
   constraint PK_T_ORDER_SHEET primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Order_Sheet') and minor_id = 0)
begin 
