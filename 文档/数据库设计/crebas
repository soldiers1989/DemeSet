/*==============================================================*/
/* Table: t_Course_Info                                         */
/*==============================================================*/
create table t_Course_Info (
   Id                   nvarchar(36)         not null,
   CourseName           nvarchar(100)        not null,
   SubjectId            nvarchar(36)         not null,
   CourseIamge          nvarchar(200)        not null,
   CourseContent        text                 not null,
   Price                numeric(6,2)         not null,
   FavourablePrice      numeric(6,2)         not null,
   ValidityPeriod       int                  null,
   State                int                  null,
   TeacherId            nvarchar(36)         null,
   IsTop                int                  null,
   IsRecommend          int                  null,
   CourseTag            nvarchar(100)        null,
   CourseLongTime       int                  null,
   CourseWareCount      int                  null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_COURSE_INFO primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Course_Info') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
execute sp_dropextendedproperty 'MS_Description',  
   'user', @CurrentUser, 'table', 't_Course_Info' 
 
end 


select @CurrentUser = user_name() 
execute sp_addextendedproperty 'MS_Description',  
   '课程表', 
   'user', @CurrentUser, 'table', 't_Course_Info'
go

