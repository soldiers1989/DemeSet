/*==============================================================*/
/* Table: t_Subject_BigQuestion                                 */
/*==============================================================*/
create table t_Subject_BigQuestion (
   Id                   nvarchar(36)         not null,
   QuestionTitle        nvarchar(100)        not null,
   QuestionContent      text                 not null,
   Option1              text                 null,
   Option2              text                 null,
   Option3              text                 null,
   Option4              text                 null,
   Option5              text                 null,
   Option6              text                 null,
   Option7              text                 null,
   Option8              text                 null,
   SubjectId            nvarchar(36)         not null,
   KnowledgePointId     nvarchar(36)         not null,
   SubjectType          nvarchar(36)         not null,
   SubjectClassId       nvarchar(36)         null,
   RightAnswer          nvarchar(50)         null,
   ConsultAnswer        text                 null,
   State                int                  not null,
   QuestionTextAnalysis text                 null,
   QuestionAudioAnalysis nvarchar(200)        null,
   QuestionVedioAnalysis nvarchar(200)        null,
   UseTimes             int                  null,
   DigitalBookPage      nvarchar(50)         null,
   DifficultLevel       int                  not null,
   AddTime              datetime             not null,
   AddUserId            nvarchar(36)         not null,
   UpdateTime           datetime             null,
   UpdateUserId         nvarchar(36)         null,
   IsDelete             int                  not null,
   constraint PK_T_SUBJECT_BIGQUESTION primary key (Id)
)
go

if exists (select 1 from  sys.extended_properties
           where major_id = object_id('t_Subject_BigQuestion') and minor_id = 0)
begin 
   declare @CurrentUser sysname 
select @CurrentUser = user_name() 
execute sp_dropextendedproperty 'MS_Description',  
   'user', @CurrentUser, 'table', 't_Subject_BigQuestion' 
 
end 


select @CurrentUser = user_name() 
execute sp_addextendedproperty 'MS_Description',  
   '试题大表', 
   'user', @CurrentUser, 'table', 't_Subject_BigQuestion'
go

