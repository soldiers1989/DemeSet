/*==============================================================*/
create table dbo.t_Base_OperatorLog (
   Id                   NVARCHAR(36)         not null,
   ModuleId             int                  not null,
   KeyId                NVARCHAR(36)         not null,
   OperatorName         NVARCHAR(20)         collate Chinese_PRC_CI_AS not null,
   OperatorDate         datetime             not null,
   OperatorType         int                  null,
   OperatorId           NVARCHAR(36)         null,
   Remark               NVARCHAR(1000)       collate Chinese_PRC_CI_AS null,
   OperatorIp           NVARCHAR(20)         collate Chinese_PRC_CI_AS null
)
on "PRIMARY"
go

