
@{
    ViewBag.Title = "Index";
    Layout = null;
}


<div class="col-xs-12">
    <div class="box-header nav-header">
        <span class="title"><i class="fa fa-map-marker map-loc"></i>位置：<a>系统管理</a>/<a>操作员日志</a></span>
    </div>
</div>
<div class="col-xs-12" style="background-color: #000000; height: 3px;"></div>
<div class="col-xs-12 ">
    <!-- 工具栏 -->
    <div class="content-header form-inline row">
        <form id="selectFrom" name="selectFrom">
            <div class="form-group col-xs-12">
                <label class="">操作类型：</label>
                <select style="width: 179px;" class="form-control" id="OperatorType" Name="OperatorType"></select>
                <label class="">模块名称：</label>
                <select  style="width: 179px;" class="form-control" id="ModuleType" Name="ModuleType"></select>
                <label class="">操作内容：</label>
                <input class="form-control" id="Content" Name="Content"/>
            </div>
            <div class="form-group col-xs-12" style="margin-top: 8px;">
                <label class="">操作人：</label>
                <input class="form-control" id="OperatorName" Name="OperatorName" />
                <label>操作时间起：</label>
                <input class="form-control Wdate" onFocus="WdatePicker({ el: 'BeginDateTime', dateFmt: 'yyyy-MM-dd', maxDate: '#F{$dp.$D(\'EndDateTime\')}' })" id="BeginDateTime" name="BeginDateTime" style="height: 34px;"/>
                <label>操作时间起：</label>
                <input class="form-control Wdate" onFocus="WdatePicker({ el: 'EndDateTime', dateFmt: 'yyyy-MM-dd', minDate: '#F{$dp.$D(\'BeginDateTime\')}' })" id="EndDateTime" name="EndDateTime" style="height: 34px;"/>
                <input class="btn btn-success query_btn" type="button" value="查询">
            </div>
        </form>
    </div>
    <div class="box-body row">
        <table id="tblData"></table>
        <div id="pager"></div>
    </div>
</div>
<style>
    #selectFrom label {
        min-width: 100px !important;
        text-align: left;
    }
</style>
<script src="~/Libs/My97DatePicker/WdatePicker.js"></script>
<script src="~/Views/OperatorLog/Index.js"></script>