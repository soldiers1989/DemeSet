
@{
    ViewBag.Title = "Index";
    Layout = null;
}


<div class="col-xs-12">
    <div class="box-header nav-header">
        <span class="title"><i class="fa fa-map-marker map-loc"></i>位置：<a>系统管理</a>/<a>数据字典</a></span>
    </div>
</div>
<div class="col-xs-12" style="background-color: #000000; height: 3px;"></div>
<div class="col-xs-12 " id="app">
    <!-- 工具栏 -->
    <div class="content-header form-inline row">
        <form id="selectFrom" name="selectFrom">
            <div class="form-group col-xs-12">
                <input class="btn btn-success add_btn" v-on:click="add" type="button" value="添加">&nbsp;
                <input class="btn btn-info edit_btn" v-on:click="edit" type="button" value="编辑">&nbsp;
                <input class="btn btn-danger del_btn" v-on:click="del" type="button" value="删除">&nbsp;

                <label class="">数据类型代码：</label>
                <input class="form-control" id="QueryCode" name="Code"/>
                <label class="">数据名称：</label>
                <input class="form-control" id="QueryName" Name="Name"/>
                <label class="">分类名称：</label>
                <select style="width: 179px;" class="form-control" id="QueryDataTypeId" Name="DataTypeId"></select>
                <input class="btn btn-success query_btn" v-on:click="query"  type="button" value="查询">&nbsp;
            </div>
        </form>
    </div>
    <div class="box-body row">
        <table id="tblData"></table>
        <div id="pager"></div>
    </div>
</div>
<script src="~/Views/BaseData/Index.js"></script>
