
@{
    ViewBag.Title = "项目新增(修改)";
}

<div class="box">
    <form id="sumbitForm" name="sumbitForm">
        <div class="box-body">
            <input type="hidden" Id="Id" name="Id" value="@ViewBag.Id" />
            <div class="col-xs-12">
                <div class="form-group">
                    <label>公司名称</label><span class="text-danger">*</span>
                    <input type="text" class="form-control" id="Name" name="Name">
                </div>
                <div class="form-group">
                    <label>联系人</label><span class="text-danger">*</span>
                    <input type="text" class="form-control" id="TheContact" name="TheContact">
                </div>
                <div class="form-group">
                    <label>提成比例</label><span class="text-danger">*</span>
                    <input type="number" min="0" step="0.01" max="99" class="form-control" id="CommissionRatio" name="CommissionRatio">
                </div>
                <div class="form-group">
                    <label>联系方式</label>
                    <input type="text" class="form-control" id="Contact" name="Contact">
                </div>
                <div class="form-group">
                    <label>公司地址</label>
                    <input type="text" class="form-control" id="Address" name="Address">
                </div>
            </div>
            <div class="box-footer col-xs-12">
                <button type="submit" class="btn btn-primary" id="sumbit">保存</button>
            </div>
        </div>
    </form>
</div>
<script src="~/Views/PromoteCompany/AddOrEdit.js"></script>
