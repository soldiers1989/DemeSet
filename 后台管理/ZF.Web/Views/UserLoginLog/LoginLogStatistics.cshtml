@{
    Layout = null;
}
<style>
    #selectFrom .form-control {
        min-width: 179px;
    }
</style>

<div class="col-xs-12">
    <div class="box-header nav-header">
        <span class="title"><i class="fa fa-map-marker map-loc"></i>位置：<a>系统模块</a>/<a>用户登陆日志</a></span>
    </div>
</div>
<div class="col-xs-12" style="background-color: #000000; height: 3px;"></div>
<div class="col-xs-12">
    <!-- 工具栏 -->
    <div class="content-header form-inline row">
        <form id="selectFrom" name="selectFrom">
            <div class="form-group">
                <label>登陆起止时间：</label>
                <input class="form-control Wdate" onFocus="WdatePicker({ el: 'BeginDateTime', dateFmt: 'yyyy-MM-dd', minDate: '#F{$dp.$D(\'EndDateTime\')}' })" id="BeginDateTime" name="BeginDateTime" style="height: 34px;" />
                -
                <input class="form-control Wdate" onFocus="WdatePicker({ el: 'EndDateTime', dateFmt: 'yyyy-MM-dd', minDate: '#F{$dp.$D(\'BeginDateTime\')}' })" id="EndDateTime" name="EndDateTime" style="height: 34px;" />
                <input class="btn btn-success query_btn" type="button" value="查询">
            </div>
        </form>
    </div>
    <div class="box-body row">
        <table id="tblData"></table>
        <div id="pager"></div>
    </div>
</div>
<script src="~/Views/UserLoginLog/PersonalLgoinLog.js"></script>