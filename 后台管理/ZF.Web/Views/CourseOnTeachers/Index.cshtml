
@{
    ViewBag.Title = "主讲教师管理";
    Layout = null;
}

<link href="~/Libs/select2-4.0.6/css/select2.min.css" rel="stylesheet" />
<div class="drop_list">
    <div class="col-xs-12">
        <div class="box-header nav-header">
            <span class="title"><i class="fa fa-map-marker map-loc"></i>位置：<a>课程管理</a>/<a>主讲教师管理</a></span>
        </div>
    </div>
    <div class="col-xs-12" style="background-color: #000000; height: 3px;"></div>
    <div class="col-xs-12">
        <!-- 工具栏 -->
        <div class="content-header form-inline row">
            <form id="selectFrom" name="selectFrom">
                <div class="form-group col-xs-12">
                    <label class="">教师名称：</label>
                    <select class="form-control" id="QueryTeachersName" style="width:150px;" Name="TeachersName"></select>
                    <label class="">所属项目：</label>
                    <select class="form-control" id="QueryProjectId" style="width:150px;" Name="ProjectId"></select>
                    <input class="btn btn-success add_btn" type="button" value="添加">&nbsp;
                    <input class="btn btn-info edit_btn" type="button" value="编辑">&nbsp;
                    <input class="btn btn-danger del_btn" type="button" value="删除">&nbsp;
                    <input class="btn btn-success query_btn" type="button" value="查询">&nbsp;
                </div>
            </form>
        </div>
        <div class="box-body row">
            <table id="tblData"></table>
            <div id="pager"></div>
        </div>
    </div>
</div>
<script src="~/Libs/select2-4.0.6/js/select2.min.js"></script>
<script src="~/Libs/select2-4.0.6/js/i18n/zh-CN.js"></script>
<script src="~/Views/CourseOnTeachers/Index.js"></script>
