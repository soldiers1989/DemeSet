
@{
    Layout = null;
}
<div class="box">
    <form id="sumbitForm" name="sumbitForm">
        <div class="box-body">
            <input type="hidden" Id="Id" name="Id" value="@ViewBag.gid" />
            <div class="col-xs-12">
                <div class="form-group">
                    <label>快递公司名称</label><span class="text-danger">*</span>
                    <input type="text" class="form-control" id="Name" name="Name">
                </div>
                <div class="form-group">
                    <label>快递公司网址</label><span class="text-danger">*</span>
                    <input type="text" class="form-control" id="Companyurl" name="Companyurl">
                </div>
            </div>
            <div class="box-footer col-xs-12">
                <button type="submit" class="btn btn-primary" id="sumbit">保存</button>
            </div>
        </div>
    </form>
</div>
<script src="~/Views/HandOutManage/AddOrEdit.js"></script>