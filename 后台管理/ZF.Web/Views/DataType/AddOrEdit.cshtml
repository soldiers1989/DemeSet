
@{
    ViewBag.Title = "字典分类(修改)";
}

<div class="box">
    <form id="sumbitForm" name="sumbitForm">
        <div class="box-body">
            <input type="hidden" Id="Id" name="Id" value="@ViewBag.Id" />
            <div class="col-xs-12">
                <div class="form-group">
                    <label>分类名称</label><span class="text-danger">*</span>
                    <input type="text" class="form-control" id="DataTypeName" name="DataTypeName">
                </div>
                <div class="form-group">
                    <label>分组代码</label><span class="text-danger">*</span>
                    <input type="text" class="form-control" id="DataTypeCode" name="DataTypeCode">
                </div>
                <div class="form-group">
                    <label>排序号</label>
                    <input type="number" class="form-control" id="Sort" name="Sort">
                </div>
                <div class="form-group">
                    <label>说明</label>
                    <textarea type="text" class="form-control" style="height: 60px;" id="Desc" name="Desc"></textarea>
                </div>
            </div>
            <div class="box-footer col-xs-12">
                <button type="submit" class="btn btn-primary" id="sumbit">保存</button>
            </div>
        </div>
    </form>
</div>
<script src="~/Views/DataType/AddOrEdit.js"></script>
