@{
    ViewBag.Title = "Index";
}


<div class="col-xs-12">
    <div class="box-header nav-header">
        <span class="title"><i class="fa fa-map-marker map-loc"></i>位置：<a>试题管理</a>/<a>错题反馈</a></span>
    </div>
</div>
<div class="col-xs-12" style="background-color: #000000; height: 3px;"></div>
<div class="col-xs-10" style="border-left-style: solid; border-width: 1px;">
    <!-- 工具栏 -->
    <div class="content-header form-inline row">
        <form id="selectFrom" name="selectFrom">
            <div class="form-group">
                <label class="">时间范围：</label>
                <input class="form-control Wdate" onFocus="WdatePicker({ el: 'QBeginDate', dateFmt: 'yyyy-MM-dd', maxDate: '#F{$dp.$D(\'QEndDate\')}' })" id="QBeginDate" name="BeginDate" style="height: 34px; width: 179px;" />至
                <input class="form-control Wdate" onFocus="WdatePicker({ el: 'QEndDate', dateFmt: 'yyyy-MM-dd', minDate: '#F{$dp.$D(\'QBeginDate\')}' })" id="QEndDate" name="EndDate" style="height: 34px; width: 179px;" />
                <label class="">状态</label>
                <input name="Audit" type="radio" checked="checked" value="0"/>待确认
                <input name="Audit" type="radio" value="1" />已确认
                <input class="btn btn-success query_btn" type="button" value="查询">&nbsp;
            </div>
        </form>
    </div>
    <div class="box-body row">
        <table id="tblData"></table>
        <div id="pager"></div>
    </div>
</div>
<script src="~/Views/SubjectBigQuestion/ErrorFeedback.js"></script>